    <function name="Main" type="None" variable="">
        <parameters/>
        <body>
            <comment text="Declaring the variables"/>
            <declare name="array" type="Integer" array="True" size="10"/>
            <declare name="input, x" type="Integer" array="False" size=""/>
            <comment text="Using the for loop for repeating the process"/>
            <for variable="x" start="0" end="9" direction="inc" step="1">
                <output expression="&quot;Enter &quot;&amp;x+1&amp;&quot; integer value&quot;" newline="True"/>
