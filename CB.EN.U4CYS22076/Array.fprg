            </for>
            <for variable="x" start="0" end="9" direction="inc" step="1">
                <comment text="Displaying all values all at a time."/>
                <output expression="&quot;The value of&quot;&amp;x+1&amp;&quot;integer is&quot;&amp;array[x]" newline="True"/>
